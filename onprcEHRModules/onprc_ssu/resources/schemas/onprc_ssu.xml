<tables xmlns="http://labkey.org/data/xml">
    <table tableName="schedule" tableDbType="TABLE" useColumnOrder="true">
        <tableTitle>SSU Schedule</tableTitle>
        <javaCustomizer class="org.labkey.ehr.table.WrappingTableCustomizer" />
        <columns>
            <column columnName="rowid">
                <columnTitle>Row Id</columnTitle>
                <isHidden>true</isHidden>
            </column>
            <column columnName="Id">
                <conceptURI>http://cpas.labkey.com/Study#ParticipantId</conceptURI>
            </column>
            <column columnName="date">
                <columnTitle>Date</columnTitle>
            </column>
            <column columnName="project">
                <columnTitle>Project Id</columnTitle>
            </column>
            <column columnName="location">
                <columnTitle>Location</columnTitle>
            </column>
            <column columnName="procedureid">
                <columnTitle>Procedure Id</columnTitle>
            </column>
            <column columnName="formcreated">
                <columnTitle>Surgery Created?</columnTitle>
                <shownInInsertView>false</shownInInsertView>
            </column>
            <column columnName="encounterid">
                <columnTitle>Surgery Id</columnTitle>
                <shownInInsertView>false</shownInInsertView>
                <shownInUpdateView>false</shownInUpdateView>
                <isUserEditable>false</isUserEditable>
            </column>
            <column columnName="taskid">
                <columnTitle>Task Id</columnTitle>
                <shownInInsertView>false</shownInInsertView>
                <shownInUpdateView>false</shownInUpdateView>
                <isUserEditable>false</isUserEditable>
            </column>
            <column columnName="objectid">
                <columnTitle>Key</columnTitle>
                <isHidden>true</isHidden>
            </column>
            <column columnName="container"/>
            <column columnName="createdby"/>
            <column columnName="created"/>
            <column columnName="modifiedby"/>
            <column columnName="modified"/>
        </columns>
    </table>
</tables>