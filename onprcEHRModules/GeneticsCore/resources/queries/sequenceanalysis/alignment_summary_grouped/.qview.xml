<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="analysis_id" />
       <!--<column name="rowids"/>-->
       <column name="analysis_id/readset">
           <properties>
               <property name="columnTitle" value="Readset Name"/>
           </properties>
       </column>
       <column name="loci"/>
       <column name="alleles">
           <properties>
               <property name="columnTitle" value="Alleles"/>
           </properties>
       </column>
       <column name="lineages">
           <properties>
               <property name="columnTitle" value="Lineages"/>
           </properties>
       </column>
       <column name="total_reads" />

       <column name="total_reads_in_analysis"/>
       <!--<column name="percent"/>-->
       <column name="total_reads_from_locus" />
       <column name="percent_from_locus"/>
       <column name="cachedHaplotypes"/>
       <column name="analysis_id/workbook">
           <properties>
               <property name="columnTitle" value="Workbook"/>
           </properties>
       </column>
       <column name="edit"/>
   </columns>
    <!--<aggregates>-->
        <!--<aggregate column="total" type="SUM"/>-->
    <!--</aggregates>-->
   <sorts>
       <sort column="analysis_id/readset"/>
       <sort column="analysis_id"/>
       <sort column="loci"/>
       <sort column="lineages"/>
       <sort column="-percent_from_locus"/>
   </sorts>
</customView>