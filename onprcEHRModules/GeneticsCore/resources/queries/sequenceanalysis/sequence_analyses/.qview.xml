<customView xmlns="http://labkey.org/data/xml/queryCustomView" canOverride="true">
   <columns>
       <column name="rowid" />
       <column name="description" />
       <column name="type" />
       <column name="readset" />
       <column name="readset/subjectid" >
           <properties>
               <property name="columnTitle" value="Subject Id"/>
           </properties>
       </column>
       <column name="readset/sampledate">
           <properties>
               <property name="columnTitle" value="Sample Date"/>
           </properties>
       </column>
       <column name="alignmentfile" />
       <column name="library_id" />
       <column name="runid"/>
       <column name="runid/JobId">
           <properties>
               <property name="columnTitle" value="Job Id"/>
           </properties>
       </column>
       <column name="created" />
       <column name="workbook" />
       <column name="readset/status">
           <properties>
               <property name="columnTitle" value="Readset Status"/>
           </properties>
       </column>
       <column name="readset/totalForwardReads"/>
       <column name="totalSbtReads"/>
       <column name="pctUnmappedSbtReads"/>
       <column name="numCachedResults"/>
       <column name="numCachedHaplotypes"/>
       <column name="numReadsDisabled"/>
       <column name="readset/instrument_run_id"/>
   </columns>
   <sorts>
       <sort column="rowid" descending="true"/>
   </sorts>
</customView>
