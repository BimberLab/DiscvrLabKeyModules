<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="project"/>
        <column name="title"/>
        <column name="project/protocol">
            <properties>
                <property name="columnTitle" value="IACUC Protocol"/>
            </properties>
        </column>
        <column name="project/protocol/investigatorId">
            <properties>
                <property name="columnTitle" value="Protocol Investigator"/>
            </properties>
        </column>
        <column name="project/protocol/investigatorId/financialAnalyst">
            <properties>
                <property name="columnTitle" value="Financial Analyst"/>
            </properties>
        </column>
        <column name="summaryByInvoice"/>
        <column name="allItems"/>
    </columns>
    <sorts>
        <sort column="project/displayName"/>
    </sorts>
</customView>