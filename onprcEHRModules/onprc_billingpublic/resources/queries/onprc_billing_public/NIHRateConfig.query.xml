<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="NIHRateConfig" tableDbType="TABLE">
                <javaCustomizer class="org.labkey.ehr.table.DefaultEHRCustomizer" />
                <tableTitle>NIH Project Rate Config</tableTitle>
                <columns>

                        <column columnName="category"></column>
                        <column columnName="ItemCode"></column>
                        <column columnName="name"></column>
                        <column columnName="UnitCost">
                            <columnTitle>Current Year</columnTitle>
                            <formatString>$#,##0.00</formatString>
                        </column>

                        <column columnName="year1">
                            <columnTitle>5/1/2017 to 4/30/2018</columnTitle>
                            <formatString>$###0.00</formatString>
                        </column>
                        <column columnName="year2">
                            <columnTitle>5/1/2018 to 4/30/2019</columnTitle>
                            <formatString>$###0.00</formatString>
                        </column>
                        <column columnName="year3">
                            <columnTitle>5/1/2019 to 4/30/2020</columnTitle>
                            <formatString>$###0.00</formatString>
                        </column>
                        <column columnName="year4">
                            <columnTitle>5/1/2020 to 4/30/2021</columnTitle>
                            <formatString>$###0.00</formatString>
                        </column>
                        <column columnName="year5">
                            <columnTitle>5/1/2021 to 4/30/2022</columnTitle>
                            <formatString>$###0.00</formatString>
                        </column>
                        <column columnName="year6">
                            <columnTitle>5/1/2022 to 4/30/2023</columnTitle>
                            <formatString>$###0.00</formatString>
                        </column>
                        <column columnName="year7">
                            <columnTitle>5/1/2023 to 4/30/2024</columnTitle>
                            <formatString>$###0.00</formatString>
                        </column>

                        <column columnName="year8">
                            <columnTitle>5/1/2024 to 4/30/2025</columnTitle>
                            <formatString>$###0.00</formatString>
                        </column>

                        <column columnName="PostedDate">
                            <columnTitle>Posted Date</columnTitle>
                            <formatString>MM/dd/yyyy</formatString>
                        </column>

                    </columns>
            </table>
        </tables>
    </metadata>
</query>