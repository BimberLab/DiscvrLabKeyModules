<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="All Grant Projects">
    <columns>
        <column name="projectNumber"/>
        <column name="grantNumber"/>
        <column name="investigatorName"/>
        <column name="projectTitle"/>
        <column name="projectDescription"/>
        <column name="fundingAgency"/>
        <column name="grantType"/>
        <column name="organization"/>
        <column name="budgetStartDate"/>
        <column name="budgetEndDate"/>
        <column name="projectStatus"/>
    </columns>
    <sorts>
        <sort column="projectNumber" descending="false"/>
    </sorts>
</customView>