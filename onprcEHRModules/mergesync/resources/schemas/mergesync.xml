<tables xmlns="http://labkey.org/data/xml">
    <table tableName="testnamemapping" tableDbType="TABLE">
        <javaCustomizer class="org.labkey.ehr.table.DefaultEHRCustomizer" />
        <tableTitle>LabKey to Merge Service/Panel Name Mapping</tableTitle>
        <columns>
            <column columnName="servicename">
                <columnTitle>Service Name</columnTitle>
                <isKeyField>true</isKeyField>
            </column>
            <column columnName="mergetestname">
                <columnTitle>Merge Test Name</columnTitle>
            </column>
            <column columnName="automaticresults">
                <columnTitle>Expect Results From Merge?</columnTitle>
            </column>
        </columns>
    </table>
    <table tableName="mergetolkmapping" tableDbType="TABLE">
        <javaCustomizer class="org.labkey.ehr.table.DefaultEHRCustomizer" />
        <tableTitle>Merge to LabKey Service/Panel Name Mapping</tableTitle>
        <columns>
            <column columnName="mergetestname">
                <columnTitle>Merge Test Name</columnTitle>
            </column>
            <column columnName="servicename">
                <columnTitle>Service Name</columnTitle>
            </column>
        </columns>
    </table>
    <table tableName="orderssynced" tableDbType="TABLE">
        <javaCustomizer class="org.labkey.ehr.table.DefaultEHRCustomizer" />
        <tableTitle>Orders Synced To/From Merge</tableTitle>
        <columns>
            <column columnName="runid">
                <columnTitle>Clinpath Runs Record</columnTitle>
            </column>
            <column columnName="requestid">
                <columnTitle>Request Id</columnTitle>
            </column>
            <column columnName="taskid">
                <columnTitle>Task Id</columnTitle>
            </column>
            <column columnName="objectid">
                <columnTitle>Key</columnTitle>
            </column>
            <column columnName="order_accession">
                <columnTitle>Merge Order Accession #</columnTitle>
            </column>
            <column columnName="test_accession">
                <columnTitle>Merge Test Accession #</columnTitle>
            </column>
            <column columnName="merge_datecreated">
                <columnTitle>Merge Date Created</columnTitle>
            </column>
            <column columnName="merge_dateposted">
                <columnTitle>Merge Date Posted</columnTitle>
            </column>
            <column columnName="deletedate">
                <columnTitle>Date Deleted In LabKey</columnTitle>
            </column>
            <column columnName="container">
                <columnTitle>Folder</columnTitle>
            </column>
            <column columnName="created">

            </column>
            <column columnName="createdby">

            </column>
            <column columnName="resultsreceived">
                <columnTitle>Results Received?</columnTitle>
            </column>
        </columns>
    </table>
</tables>