<module xmlns="http://labkey.org/moduleProperties/xml/">
    <properties>
        <propertyDescriptor name="SLAContainer">
            <canSetPerContainer>true</canSetPerContainer>
            <description>The is the path to the container holding SLA data.  Use of slashes is very important - it should be in the format '/myProject/ehr'</description>
            <editPermissions>
                <permission>ADMIN</permission>
            </editPermissions>
        </propertyDescriptor>
    </properties>
    <requiredModuleContext>
        <requiredModule name="LDK"/>
    </requiredModuleContext>
    <clientDependencies>
        <dependency path="Ext4"/>
        <dependency path="Ext4ClientApi"/>
        <dependency path="clientapi"/>
        <dependency path="LDK.context"/>
        <!--<dependency path="sla/sla"/>-->
    </clientDependencies>
</module>
