<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsDNABank" tableDbType="NOT_IN_DB">
                <tableTitle>DNA Bank Summary</tableTitle>
                <columns>
                    <column columnName="totalSamples">
                        <url>/query/executeQuery.view?schemaName=DNA_Bank&amp;query.queryName=samples&amp;query.subjectid~eq=${Id}</url>
                    </column>
                    <column columnName="sampleTypes">
                        <url>/query/executeQuery.view?schemaName=DNA_Bank&amp;query.queryName=samples&amp;query.subjectid~eq=${Id}</url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
