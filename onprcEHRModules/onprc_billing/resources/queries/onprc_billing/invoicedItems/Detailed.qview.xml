<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="invoiceId"/>
        <column name="transactionNumber"/>
        <column name="category"/>
        <column name="item"/>
        <column name="itemCode"/>
        <column name="Id"/>
        <column name="date"/>
        <column name="project"/>
        <column name="debitedaccount"/>
        <column name="creditedaccount"/>
        <column name="investigatorId"/>
        <column name="faid"/>
        <column name="unitCost"/>
        <column name="quantity"/>
        <column name="totalcost"/>
        <column name="comment"/>
        <column name="chargeCategory"/>
        <column name="billingdate"/>

        <column name="rateId"/>
        <column name="exemptionId"/>

    </columns>
    <sorts>
        <sort column="invoiceId/rowid" descending="true"/>
        <sort column="Id"/>
        <sort column="date" descending="true"/>
    </sorts>
    <aggregates>
        <aggregate column="total" type="SUM"/>
    </aggregates>
</customView>