<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>Project Charge</name>
<description>Project Charge Combinations</description>
<transforms>
    <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
        <description>Copy to target</description>
        <source schemaName="cnprcSrc_billing" queryName="q_project_charge"/>
        <destination schemaName="cnprc_billing" queryName="project_charge" bulkLoad="true" targetOption="merge">
            <alternateKeys>
                <column name="objectid"/>
            </alternateKeys>
        </destination>
    </transform>
</transforms>
    <incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="date_time">
        <deletedRowsSource schemaName="cnprcSrc_billing_aud" queryName="q_project_charge_delete" timestampColumnName="date_time"
                           deletedSourceKeyColumnName="objectid" targetKeyColumnName="objectid"/>
    </incrementalFilter>
<schedule>
    <poll interval="60m" />
</schedule>
</etl>
