<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Per Diem - Month</name>
    <description>Per Diem Month</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc_billing_fin" queryName="q_perdiem_month" />
            <destination schemaName="cnprc_billing" queryName="perdiem_month" bulkLoad="true" batchSize="10000" targetOption="merge">
                <alternateKeys>
                    <column name="objectid"/>
                </alternateKeys>
            </destination>
        </transform>
    </transforms>
    <incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="date_time"/>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>