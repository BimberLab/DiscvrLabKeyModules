<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Work Orders</name>
    <description>All work orders - Fiscal year to date</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc_billing" queryName="q_work_order" />
            <destination schemaName="cnprc_billing" queryName="work_order" bulkLoad="true" batchSize="5000" targetOption="truncate"/>
        </transform>
        <transform id="step2" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc_billing" queryName="q_wo_recipients" />
            <destination schemaName="cnprc_billing" queryName="wo_recipients" bulkLoad="true" batchSize="5000" targetOption="truncate"/>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>