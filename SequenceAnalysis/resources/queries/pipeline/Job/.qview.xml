<customView xmlns="http://labkey.org/data/xml/queryCustomView" canOverride="true">
    <columns>
        <column name="Status" />
        <column name="Info" />
        <column name="Description" />
        <column name="ActiveHostName"/>
        <column name="Created" />
        <column name="CreatedBy" />
        <column name="Folder/Path">
            <properties>
                <property name="columnTitle" value="Folder"/>
            </properties>
        </column>
        <column name="RowId" />
    </columns>
    <sorts>
        <sort column="Created" descending="true"/>
    </sorts>
</customView>