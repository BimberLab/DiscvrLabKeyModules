<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">           
        <table tableName="sequence_analyses" tableDbType="TABLE">
            <!--<pkColumnName>rowid</pkColumnName>-->
            <titleColumn>rowid</titleColumn>
            <columns>
              <column columnName="runid">
                <url>/experiment/showRunGraphDetail.view?rowId=${runid}</url>
              </column>
            </columns>
            <buttonBarOptions includeStandardButtons="false">
                <includeScript>/sequenceanalysis/sequenceanalysisButtons.js</includeScript>
                <item text="Views">
                    <originalText>Views</originalText>
                </item>
                <item text="Delete" permission="DELETE" requiresSelection="true">
                    <onClick>SequenceAnalysis.Buttons.deleteTable(dataRegionName);</onClick>
                </item>
                <item text="Export">
                    <originalText>Export</originalText>
                </item>
                <item text="Print">
                    <originalText>Print</originalText>
                </item>
                <item text="Page Size">
                    <originalText>Page Size</originalText>
                </item>
                <item insertPosition="end" requiresSelection="true" text="More Actions">
                    <item text="Re-analyze Selected">
                        <onClick>SequenceAnalysis.Buttons.performAnalysis(dataRegionName, arguments[0] ? arguments[0].ownerCt.floatParent : null)</onClick>
                    </item>
                    <item text="View FASTQC Report">
                        <onClick>SequenceAnalysis.Buttons.generateFastQcForAnalysis(dataRegionName, arguments[0] ? arguments[0].ownerCt.floatParent : null);</onClick>
                    </item>
                    <item text="Download Sequence Files">
                        <onClick>SequenceAnalysis.Buttons.downloadFilesForAnalysis(dataRegionName, arguments[0] ? arguments[0].ownerCt.floatParent : null);</onClick>
                    </item>
                    <item text="Alignment Reports">
                        <item text="Alignment Summary">
                            <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'alignment_summary_grouped'})</onClick>
                        </item>
                        <item text="Alignment Summary, By Lineage">
                            <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'alignment_summary_by_lineage'})</onClick>
                        </item>
                        <item text="Alignment Summary, Pivoted By Lineage">
                            <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'alignment_summary_by_lineage_pivoted'})</onClick>
                        </item>
                        <item text="Matching Haplotypes">
                            <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'haplotypeMatches'})</onClick>
                        </item>
                        <item text="View Coverage">
                            <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'sequence_coverage'})</onClick>
                        </item>
                    </item>
                    <item text="NT SNP Reports">
                        <!--<item text="View All NT SNPs">-->
                            <!--<onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'nt_snps'})</onClick>-->
                        <!--</item>-->
                        <item text="NT Mutation Summary, By Position and Base">
                            <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'nt_snps_by_pos'})</onClick>
                        </item>
                        <!--<item text="NT Mutation Summary, By Position and Base">-->
                            <!--<onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'nt_snp_by_pos_base'})</onClick>-->
                        <!--</item>-->
                    </item>
                    <item text="AA SNP Reports">
                        <!--<item text="View All AA SNPs">-->
                            <!--<onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'aa_snps'})</onClick>-->
                        <!--</item>-->
                        <!--<item text="AA Mutation Summary, By Position">-->
                            <!--<onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'aa_snp_by_pos'})</onClick>-->
                        <!--</item>-->
                        <item text="AA Mutation Summary, By Position and Codon">
                            <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'aa_snps_by_codon'})</onClick>
                        </item>
                        <item text="Predicted Drug Resistance Mutations">
                            <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {queryName: 'drug_resistance_snps'})</onClick>
                        </item>
                        <item text="SNP Viewer">
                            <onClick>SequenceAnalysis.Buttons.viewSNPs(dataRegionName)</onClick>
                        </item>
                        <!--<item text="SNP Haplotype Viewer">-->
                            <!--<onClick>SequenceAnalysis.Buttons.viewSNPDetails(dataRegionName)</onClick>-->
                        <!--</item>-->
                    </item>
                    <item text="Publish Records">
                            <onClick>SequenceAnalysis.Buttons.publishAnalyses(dataRegionName, arguments[0] ? arguments[0].ownerCt.floatParent : null);</onClick>
                    </item>
                </item>
            </buttonBarOptions>
        </table>
    </tables>
    </metadata>
</query>
