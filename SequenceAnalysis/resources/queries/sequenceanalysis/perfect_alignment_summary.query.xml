<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="perfect_alignment_summary" tableDbType="TABLE">
                <tableTitle>Summary of Perfect Alignments</tableTitle>
                <columns>
                      <column columnName="total">
                          <columnTitle>Total Reads</columnTitle>
                          <url>/query/executeQuery.view?
                              schemaName=sequenceanalysis&amp;
                              query.analysis_id~eq=${analysis_id}&amp;
                              query.queryName=perfect_alignments_by_read&amp;
                              query.Refs~eq=${Refs}&amp;
                          </url>
                      </column>
                    <column columnName="analysis_id">
                        <fk>
                          <fkDbSchema>sequenceanalysis</fkDbSchema>
                          <fkTable>sequence_analyses</fkTable>
                          <fkColumnName>rowid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="keyField">
                        <isHidden>true</isHidden>
                        <isKeyField>true</isKeyField>
                    </column>
                    <column columnName="ref_ids">
                        <isHidden>true</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
