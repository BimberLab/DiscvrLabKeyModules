<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="sequence_analysis_summary" tableDbType="TABLE">
                <tableTitle>Summary of Sequence Analysis Results</tableTitle>
                <columns>
                      <column columnName="rowid">
                          <isHidden>true</isHidden>
                      </column>
                      <column columnName="Total_Alignments">
                          <columnTitle>Total Alignments</columnTitle>
                          <url>/query/executeQuery.view?
                              schemaName=sequenceanalysis&amp;
                              query.queryName=sequence_alignments&amp;
                              query.analysis_id~eq=${rowid}&amp;
                          </url>
                      </column>
                    <column columnName="Total_NT_SNPs">
                        <columnTitle>Total NT SNPs</columnTitle>
                        <url>/query/executeQuery.view?
                            schemaName=sequenceanalysis&amp;
                            query.queryName=NT_SNPs&amp;
                            query.analysis_id~eq=${rowid}&amp;
                        </url>
                    </column>
                    <column columnName="Total_AA_SNPs">
                        <columnTitle>Total AA SNPs</columnTitle>
                        <url>/query/executeQuery.view?
                            schemaName=sequenceanalysis&amp;
                            query.queryName=AA_SNPs&amp;
                            query.analysis_id~eq=${rowid}&amp;
                        </url>
                    </column>
                    <column columnName="Total_Alignment_Summaries">
                        <columnTitle>Total Alignment Summaries</columnTitle>
                        <url>/query/executeQuery.view?
                            schemaName=sequenceanalysis&amp;
                            query.queryName=alignment_summary_grouped&amp;
                            query.analysis_id~eq=${rowid}&amp;
                        </url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
