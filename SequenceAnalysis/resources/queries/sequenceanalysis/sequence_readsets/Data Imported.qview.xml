<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="rowid" />
       <column name="name" />
       <column name="subjectid" />
       <column name="sampledate"/>
       <column name="sampleid" />
       <column name="fileid" />
       <!--<column name="qc_file" />-->
       <column name="fileid2" />
       <!--<column name="qc_file2" />-->
       <column name="barcode5" />
       <column name="barcode3" />
       <column name="platform" />
       <column name="instrument_run_id" />
       <column name="runid/RowId">
           <properties>
               <property name="columnTitle" value="Run"/>
           </properties>
       </column>
       <column name="runid/JobId">
           <properties>
               <property name="columnTitle" value="Job Id"/>
           </properties>
       </column>
       <column name="created" />
       <column name="workbook" />
   </columns>
   <sorts>
       <sort column="rowid" descending="true"/>
   </sorts>
    <filters>
        <filter column="fileid" operator="isnonblank"/>
    </filters>
</customView>
