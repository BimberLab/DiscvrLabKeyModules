<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
        <table tableName="sequence_readsets" tableDbType="TABLE">
            <columns>

            </columns>
            <buttonBarOptions includeStandardButtons="false">
                <includeScript>/sequenceanalysis/sequenceanalysisButtons.js</includeScript>
                <item text="Views">
                    <originalText>Views</originalText>
                </item>
                <item text="Import Data" permission="INSERT">
                    <originalText>Import Data</originalText>
                </item>
                <item text="Delete" permission="DELETE" requiresSelection="true">
                    <onClick>SequenceAnalysis.Buttons.deleteTable(dataRegionName);</onClick>
                </item>
                <item text="Export">
                    <originalText>Export</originalText>
                </item>
                <item text="Print">
                    <originalText>Print</originalText>
                </item>
                <item text="Page Size">
                    <originalText>Page Size</originalText>
                </item>
                <item insertPosition="end" requiresSelection="true" text="More Actions">
                    <item text="View Analyses">
                        <onClick>SequenceAnalysis.Buttons.viewQuery(dataRegionName, {keyField: 'readset', queryName: 'sequence_analyses'})</onClick>
                    </item>
                    <item text="Analyze Selected">
                        <onClick>SequenceAnalysis.Buttons.performAnalysisFromReadsets(dataRegionName, arguments[0] ? arguments[0].ownerCt.floatParent : null)</onClick>
                    </item>
                    <item text="View FASTQC Report">
                        <onClick>SequenceAnalysis.Buttons.generateFastQc(dataRegionName);</onClick>
                    </item>
                    <item text="Download Sequence Files">
                        <onClick>SequenceAnalysis.Buttons.downloadFilesForReadset(dataRegionName, arguments[0] ? arguments[0].ownerCt.floatParent : null);</onClick>
                    </item>
                    <item text="Create Illumina Sample Sheet">
                        <onClick>SequenceAnalysis.Buttons.generateIlluminaSampleFile(dataRegionName);</onClick>
                    </item>
                </item>
            </buttonBarOptions>
        </table>
    </tables>
    </metadata>
