{
  "name": "jbrowse-lk",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm ci",
    "build": "npm run build-dev",
    "start": "cross-env NODE_ENV=development LK_MODULE_CONTAINER=DiscvrLabKeyModules LK_MODULE=jbrowse webpack-dev-server --config config/watch.config.js",
    "build-no-pkg": "npm run clean && cross-env NODE_ENV=development LK_MODULE_CONTAINER=DiscvrLabKeyModules LK_MODULE=jbrowse webpack --config config/dev.config.js --progress --profile",
    "build-dev": "npm run build-no-pkg",
    "build-prod": "npm run clean && cross-env NODE_ENV=production PROD_SOURCE_MAP=source-map LK_MODULE_CONTAINER=DiscvrLabKeyModules LK_MODULE=jbrowse webpack --config config/prod.config.js --progress --profile",
    "clean": "rimraf resources/web/gen && rimraf resources/web/jbrowse/gen && rimraf resources/views/gen && rimraf resources/views/browser*",
    "test": "cross-env NODE_ENV=test jest",
    "vcffeature-hack": "node ./tools/fixVcfFeature.js",
    "postinstall": "npm run vcffeature-hack",
    "prepareCli": "rimraf ./buildCli && rimraf ./resources/external/jb-cli && npm install @jbrowse/cli@1.7.4 --prefix ./buildCli",
    "jb-pkg": "npm run prepareCli && npx pkg --outdir=./resources/external/jb-cli ./buildCli/node_modules/@jbrowse/cli && rimraf ./buildCli"
  },
  "peerDependencies": {

  },
  "dependencies": {
    "@jbrowse/cli": "^2.6.1",
    "@jbrowse/core": "^1.7.4",
    "@jbrowse/plugin-variants": "^1.7.4",
    "@jbrowse/plugin-svg": "^1.7.4",
    "@jbrowse/plugin-linear-genome-view": "^1.7.4",
    "@jbrowse/react-linear-genome-view": "^1.7.4",
    "@labkey/api": "^1.21.0",
    "@labkey/components": "^2.351.0",
    "@gmod/vcf": "^5.0.6",
    "react": "^16.14.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "jquery": "^3.6.3",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.23",
    "node-polyfill-webpack-plugin": "1.1.4",
    "path-browserify": "^1.0.1",
    "pkg": "^5.8.1",
    "react-data-grid": "^7.0.0-beta.12",
    "react-google-charts": "^3.0.15",
    "react-hot-loader": "^4.13.0",
    "regenerator-runtime": "^0.13.9",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.9.4",
    "util": "^0.12.0",
    "vm-browserify": "^1.1.2",
    "xlsx": "^0.18.5",
    "fs": "^0.0.1-security",
    "child_process": "^1.0.2",
    "esbuild": "^0.18.4"
  },
  "devDependencies": {
    "@labkey/build": "^6.12.0",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@types/jquery": "^3.5.14",
    "@types/jexl": "^2.3.0",
    "@types/node": "^18.16.18",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "jest": "^27.4.7",
    "jest-cli": "^27.4.7",
    "jest-teamcity-reporter": "^0.9.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "tls": false
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/test/styleMock.ts"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
