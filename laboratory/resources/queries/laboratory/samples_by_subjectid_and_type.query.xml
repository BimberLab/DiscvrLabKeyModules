<query xmlns="http://labkey.org/data/xml/query">
  <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="samples_by_subjectid_and_type" tableDbType="TABLE">
                <tableTitle>Samples By SubjectId And Type</tableTitle>
                <javaCustomizer>org.labkey.laboratory.query.SamplesCustomizer</javaCustomizer>
                <columns>
                    <column columnName="subjectid">
                        <isKeyField>true</isKeyField>
                    </column>
                    <column columnName="sampletype" />
                    <column columnName="total">
                        <url>/query/executeQuery.view?schemaName=laboratory&amp;query.queryName=samples&amp;query.subjectid~eq=${subjectid}&amp;query.sampletype~eq=${sampletype}</url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>