<folderType xmlns="http://labkey.org/data/xml/folderType">
    <name>Expt Workbook</name>
    <description>Workbook template for basic experiments</description>
    <requiredWebParts>
        <webPart>
            <name>Workbook Header</name>
            <location>body</location>
        </webPart>
        <webPart>
            <name>Lab Tools</name>
            <location>right</location>
        </webPart>
    </requiredWebParts>
    <preferredWebParts>
        <webPart>
            <name>Folder Summary</name>
            <location>body</location>
        </webPart>
        <webPart>
            <name>Pipeline Files</name>
            <location>body</location>
        </webPart>
        <webPart>
            <name>Messages</name>
            <location>body</location>
            <property name="boardName" value="Comments" />
            <property name="conversationName" value="Comment" />
        </webPart>
    </preferredWebParts>
    <modules>
        <moduleName>Pipeline</moduleName>
        <moduleName>Experiment</moduleName>
        <moduleName>Query</moduleName>
        <moduleName>Core</moduleName>
    </modules>
    <defaultModule>Core</defaultModule>
    <workbookType>true</workbookType>
    <forceAssayUploadIntoWorkbooks>true</forceAssayUploadIntoWorkbooks>
</folderType>