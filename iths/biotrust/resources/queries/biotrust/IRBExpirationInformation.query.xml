
<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="IRBExpirationInformation" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="SurveyRowId">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="Link">
                        <isHidden>true</isHidden>
                        <url>/biotrust/manageDocumentSet.view?rowId=${SurveyRowId}</url>
                    </column>
                    <column columnName="StudyStatus">
                        <conditionalFormats>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="In Review"/>
                                </filters>
                                <textColor>FF0000</textColor>
                            </conditionalFormat>
                            <conditionalFormat>
                                <filters>
                                    <filter operator="eq" value="Active"/>
                                </filters>
                                <textColor>008000</textColor>
                            </conditionalFormat>
                        </conditionalFormats>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>