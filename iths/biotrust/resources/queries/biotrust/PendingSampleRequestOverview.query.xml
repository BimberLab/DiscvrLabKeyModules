<query xmlns="http://labkey.org/data/xml/query">
	<metadata>
	 <tables xmlns="http://labkey.org/data/xml">
	  <table tableName="PendingSampleRequestOverview" tableDbType="NOT_IN_DB">
	    <columns>
            <column columnName="NumSampleRequests">
                <columnTitle># Sample Requests</columnTitle>
                <url>/project/begin.view?pageId=${PageId}&amp;category=${Category}&amp;status=${Status}</url>
            </column>
            <column columnName="PageId">
                <isHidden>true</isHidden>
            </column>
	    </columns>
	  </table>
	 </tables>
	</metadata>
</query>