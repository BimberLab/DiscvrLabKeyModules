<ns:tables xmlns:ns="http://labkey.org/data/xml">
  <ns:table tableName="RequestCategory" tableDbType="TABLE">
      <ns:titleColumn>RowId</ns:titleColumn>
      <ns:columns>
        <ns:column columnName="RowId"/>
        <ns:column columnName="Category"/>
        <ns:column columnName="SortOrder"/>
        <ns:column columnName="CreatedBy"/>
        <ns:column columnName="Created"/>
        <ns:column columnName="ModifiedBy"/>
        <ns:column columnName="Modified"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="RequestStatus" tableDbType="TABLE">
      <ns:titleColumn>Status</ns:titleColumn>
      <ns:columns>
        <ns:column columnName="Status"/>
        <ns:column columnName="SortOrder"/>
        <ns:column columnName="LockedState"/>
        <ns:column columnName="ApprovalState"/>
        <ns:column columnName="FinalState"/>
        <ns:column columnName="CreatedBy"/>
        <ns:column columnName="Created"/>
        <ns:column columnName="ModifiedBy"/>
        <ns:column columnName="Modified"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="DocumentTypes" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="TypeId"/>
        <ns:column columnName="Name"/>
        <ns:column columnName="Description"/>
        <ns:column columnName="AllowMultipleUpload"/>
        <ns:column columnName="Expiration"/>
        <ns:column columnName="CreatedBy"/>
        <ns:column columnName="Created"/>
        <ns:column columnName="ModifiedBy"/>
        <ns:column columnName="Modified"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="RequiredDocuments" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="SurveyDesignId"/>
        <ns:column columnName="DocumentTypeId"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="SpecimenRequestDocuments" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="OwnerId"/>
        <ns:column columnName="OwnerType"/>
        <ns:column columnName="DocumentTypeId"/>
        <ns:column columnName="AttachmentParentId"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="SamplePickup" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="RowId"/>
        <ns:column columnName="Container"/>
        <ns:column columnName="Name"/>
        <ns:column columnName="Description"/>
        <ns:column columnName="ArrangeForPickup"/>
        <ns:column columnName="HoldOvernight"/>
        <ns:column columnName="PickupContact"/>
        <ns:column columnName="SampleTestLocation"/>
        <ns:column columnName="LabBuilding"/>
        <ns:column columnName="LabRoom"/>
        <ns:column columnName="CreatedBy"/>
        <ns:column columnName="Created"/>
        <ns:column columnName="ModifiedBy"/>
        <ns:column columnName="Modified"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="ParticipantEligibilityMap" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="TissueId"/>
        <ns:column columnName="EligibilityId"/>
        <ns:column columnName="Container"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="SamplePickupMap" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="TissueId"/>
        <ns:column columnName="PickupId"/>
        <ns:column columnName="Container"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="SampleReviewerMap" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="Container"/>
        <ns:column columnName="TissueId"/>
        <ns:column columnName="Reviewer"/>
      </ns:columns>
    </ns:table>
</ns:tables>
