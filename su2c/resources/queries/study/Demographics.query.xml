<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="" tableDbType="TABLE" useColumnOrder="true">
                <tableTitle>Patient Demographics</tableTitle>
                <javaCustomizer class="org.labkey.su2c.Su2cCustomizer" />
                <columns>
                    <column columnName="patientId">

                    </column>
                    <column columnName="study">
                        <columnTitle>Study</columnTitle>
                    </column>
                    <column columnName="date">
                        <columnTitle>Enrollment Date</columnTitle>
                    </column>
                    <column columnName="ageAtEnrollment">
                        <columnTitle>Age At Enrollment</columnTitle>
                    </column>
                    <column columnName="gender">
                        <fk>
                            <fkDbSchema>lookups</fkDbSchema>
                            <fkTable>gender</fkTable>
                            <fkColumnName>value</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="race">
                        <columnTitle>Race</columnTitle>
                    </column>
                    <column columnName="ethnicity">
