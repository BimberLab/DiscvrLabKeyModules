<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="SampleInventory" tableDbType="TABLE" useColumnOrder="true">
                <columns>
                    <column columnName="AnimalId">
                        <columnTitle>Animal Id</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>Animal</fkTable>
                            <fkColumnName>id</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="Date">
                        <columnTitle>Collection Date</columnTitle>
                    </column>
                    <column columnName="SampleId">
                        <columnTitle>Sample Id</columnTitle>
                    </column>
                    <column columnName="ObjectId">
                        <shownInInsertView>false</shownInInsertView>
                        <shownInUpdateView>false</shownInUpdateView>
                        <isUnselectable>true</isUnselectable>
                        <columnTitle>Lab</columnTitle>
                    </column>
                    <column columnName="Aim">
                        <fk>
                            <fkDbSchema>snprc_r24</fkDbSchema>
                            <fkTable>LookupAims</fkTable>
                            <fkColumnName>Value</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="SampleType">
                        <fk>
                            <fkDbSchema>snprc_r24</fkDbSchema>
                            <fkTable>LookupSampleTypes</fkTable>
                            <fkColumnName>Value</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="SampleWeight">
                        <columnTitle>Sample Weight</columnTitle>
                    </column>
                    <column columnName="SampleAmount">
                        <columnTitle>Sample Amount</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>